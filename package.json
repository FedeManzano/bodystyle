{
  "name": "bodyui2",
  "version": "6.5.0",
  "description": "Biblioteca de estilos estáticos y dinámicos para la creación del Front-End WEB.",
  "main": "dist/js/bodystyle.js",
  "types": "src/types/index.d.ts",
  "scripts": {
    "build": "webpack",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:summary": "jest --coverage --coverageReporters=\"json-summary\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FedeManzano/bodystyle"
  },
  "files": [
    "dist/{css,js}/*.{css,js,map}",
    "src/{modulos}/**/*.{js}",
    "src/**/*.js",
    "src/types/**/*.d.ts",
    "sass/**/*.scss"
  ],
  "keywords": [
    "biblioteca",
    "css",
    "framework",
    "ui",
    "sass"
  ],
  "author": "Federico Manzano",
  "license": "MIT",
  "style": "dist/css/bodystyle.css",
  "sass": "sass/bodystyle.scss",
  "dependencies": {
    "@bodystyle/show-code": "^2.0.0",
    "dytips": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^20.0.0",
    "babel": "6.23.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^30.2.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^7.1.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "style-loader": "^4.0.0",
    "typescript": "^5.3.0",
    "webpack": "^5.102.0",
    "webpack-cli": "^4.1.0"
  }
}